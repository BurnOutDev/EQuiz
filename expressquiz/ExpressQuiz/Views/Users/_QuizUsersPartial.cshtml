@model IEnumerable<ExpressQuiz.ViewModels.QuizResultViewModel>

@if (Model.Any())
{
    <br/>
    <h4 class="text-center">Best players</h4>
    <table class="table table-striped table-hover ">
        <thead>
            <tr>
                <th>User</th>
                <th>Score</th>
                <th>Time</th>

            </tr>
        </thead>

        <tbody>

            @{

    foreach (var r in Model)
    {

        <tr>


            <td>@r.UserId</td>
            <td>@r.Score</td>
            <td>@r.EllapsedTime</td>
        </tr>

    }
            }


        </tbody>
    </table>
}
